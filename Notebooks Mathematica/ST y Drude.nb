Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", 
   RowBox[{"q_", ",", "a_", ",", "b_", ",", "c_"}], "]"}], ":=", 
  RowBox[{"Sqrt", "[", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"a", "^", "2"}], "+", "q"}], ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"b", "^", "2"}], "+", "q"}], ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"c", "^", "2"}], "+", "q"}], ")"}]}], "]"}]}]], "Input",
 CellChangeTimes->{
  3.921275351489011*^9, {3.9212754556760263`*^9, 3.921275490257451*^9}, {
   3.9212755211880836`*^9, 3.921275605763112*^9}, {3.9212795871680512`*^9, 
   3.9212796313452005`*^9}, {3.9212796668823166`*^9, 
   3.9212797191010065`*^9}, {3.9212797597359266`*^9, 
   3.9212798877989902`*^9}, {3.921524278821113*^9, 3.921524281019103*^9}},
 CellLabel->"In[50]:=",ExpressionUUID->"00bcac75-399d-4d2b-b0f9-7962f6e6fb2e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"l", "[", 
   RowBox[{"j_", ",", "a_", ",", "b_", ",", "c_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", "=", " ", 
      RowBox[{"a", " ", "b", " ", "c", " ", "*", "0.5"}]}], "}"}], ",", 
    "\[IndentingNewLine]", "\t\t\t\t", 
    RowBox[{"Which", "[", 
     RowBox[{
      RowBox[{"j", "==", "1"}], ",", 
      RowBox[{"n", "*", 
       RowBox[{"NIntegrate", "[", 
        RowBox[{
         RowBox[{"1", "/", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"a", "^", "2"}], "+", "q"}], ")"}], " ", 
            RowBox[{"(", 
             RowBox[{"f", "[", 
              RowBox[{"q", ",", "a", ",", "b", ",", "c"}], "]"}], ")"}]}], 
           ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"q", ",", "0", ",", "Infinity"}], "}"}]}], "]"}]}], ",", 
      "\[IndentingNewLine]", "\t\t\t\t\t", 
      RowBox[{"j", "==", "2"}], ",", 
      RowBox[{"n", "*", 
       RowBox[{"NIntegrate", "[", 
        RowBox[{
         RowBox[{"1", "/", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"b", "^", "2"}], "+", "q"}], ")"}], " ", 
